namespace NullSpace.Communication;

enum ImuId: short {Unknown = 0, Chest = 1, Left_Forearm = 2, Left_Upper_Arm = 3, Right_Forearm = 4, Right_Upper_Arm = 5}

struct Quaternion {
	id: ImuId;
	x: float;
	y: float;
	z: float; 
	w: float;
}

table TrackingUpdate {
	quaternions:[Quaternion];

}

root_type TrackingUpdate;